{
  "project_panel": {
    "hide_gitignore": true
  },
  "features": {
    "edit_prediction_provider": "zed"
  },
  "disable_ai": false,
  "autosave": "on_focus_change",
  "icon_theme": "Zed (Default)",
  "ui_font_size": 24.0,
  "buffer_font_size": 24.0,
  "theme": {
    "mode": "dark",
    "light": "Gruvbox Light",
    "dark": "Vercel Dark"
  },
  "lsp": {
    "rust-analyzer": {
      "initialization_options": {
        "check": {
          "command": "clippy" // rust-analyzer.check.command (default: "check")
        }
      }
    }
  }
}
